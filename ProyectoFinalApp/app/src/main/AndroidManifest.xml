<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.afinal.proyecto.proyectofinalapp">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Clientes_Interface.ClientesActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Clientes_Interface.Clientes_Detalles.DetallesClientesActivity"
            android:label="@string/title_activity_detalles_clientes"
            android:theme="@style/AppTheme" />
        <activity android:name=".Formulario" />
        <activity android:name=".Tarjeta" />
        <activity
            android:name=".Clientes_Interface.Clientes_Configuracion.ConfiguracionClientesActivity"
            android:label="@string/title_activity_configuracion_clientes"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>